// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @default(uuid()) @id
  name      String  
  surname   String
  email     String  @unique
  password  String
  nickname  String  @unique

  cpf       String?  @unique
  rg        String?  @unique
  @@map("user")
}

